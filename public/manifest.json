{
  "manifest_version": 3,
  "name": "Tab Hoarder",
  "version": "0.1.0",
  "description": "Save and organize browser tabs into collections. Open-source Toby alternative.",
  "permissions": ["tabs"],
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
